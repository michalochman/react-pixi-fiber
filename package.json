{
  "name": "react-pixi-fiber",
  "version": "0.4.0",
  "description": "React Fiber renderer for PIXI.js",
  "main": "index.js",
  "author": "Michal Ochman",
  "license": "MIT",
  "repository": "github:michalochman/react-pixi-fiber",
  "typings": "./index.d.ts",
  "files": [
